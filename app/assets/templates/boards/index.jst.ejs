<div class="row">
  <h3 class="col-xs-4"><span class="glyphicon glyphicon-user"></span> My Boards</h3>
  
  <div class="new-board col-xs-6">
  </div>
  
  <div class="col-xs-1">
    <button class="btn btn-primary" 
            id="new-board"> 
    <span class="glyphicon glyphicon-plus"></span>
    </button> 
  </div>
</div>



<% boards.each(function (board){ %>
  <div class="container col-xs-3">
    <a class="" 
       href="#/boards/<%= board.get("id") %>">
      <%= board.get("title") %>
    </a>
  </div>
<% }) %>




