  <div class="panel board">
    <div class="panel-heading">
      <a class="" 
         href="#/boards/<%= board.get("id") %>">
        <h4><%= board.get("title") %></h4>
      </a>
    </div>
    <div class="panel-body">
      <div id="members">
          <%= board.members %>
          <button class="btn btn-xs glyphicon glyphicon-plus pull-right" 
                 id="new-member"></button>
      </div>
    </div>
  </div>



